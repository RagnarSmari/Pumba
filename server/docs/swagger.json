                            }
                        }
                    }
                }
            },
            "post": {
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Timestamps"
                ],
                "summary": "Creates a new timestamp",
                "parameters": [
                    {
                        "description": "Timestamp",
                        "name": "Timestamp",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/dtos.TimestampRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/dtos.TimestampDto"
