{
    "swagger": "2.0",
    "info": {
        "title": "Pumba API doc",
        "contact": {},
        "version": "1.0"
    },
    "paths": {
        "/auth/logout": {
            "post": {
                "description": "Called after a user has logged out using firebase\nOverwrites the existing cookie",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Auth"
                ],
                "summary": "Logs out a user",
                "responses": {
                    "204": {
                        "description": "No Content"
                    }
                }
            }
        },
        "/auth/newSession": {
            "post": {
                "description": "Called after a user has logged in using firebase\nCreates a secure https only cookie for user to use",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Auth"
                ],
                "summary": "Creates a new session cookie for that user",
                "parameters": [
                    {
                        "description": "Session",
                        "name": "Job",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/dtos.NewSessionRequest"
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Created"
                    }
                }
            }
        },
        "/auth/sessioncheck": {
            "get": {
                "description": "Checks if session already exists, if user is already logged in",
                "tags": [
                    "Auth"
                ],
                "summary": "Session check",
                "responses": {
                    "204": {
                        "description": "No Content"
                    }
                }
            }
        },
